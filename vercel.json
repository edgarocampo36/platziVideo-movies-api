{
  "name": "platzivideo",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@platzivideos_db_user",
    "DB_PASSWORD": "@platzivideos_db_password",
    "DB_HOST": "@platzivideos_db_host",
    "DB_NAME": "@platzivideos_db_name"
  }
}
